<?xml version="1.0" encoding="UTF-8"?>
<Installer>
	<Name>Natron Community Plugins</Name>
	<Version>1.0</Version>
	<Title>Natron Community Plugins</Title>
	<Publisher>Natron Community</Publisher>
	<!--<StartMenuDir>Natron Community Plugins</StartMenuDir>-->
	<ProductUrl>https://github.com/NatronVFX/natron-plugins</ProductUrl>
	<RemoveTargetDir>false</RemoveTargetDir>
	<AllowSpaceInPath>true</AllowSpaceInPath>
	<!-- <UninstallerName>NCP-Setup</UninstallerName> -->
	<InstallerWindowIcon>icon.png</InstallerWindowIcon>
	<Logo>logo.png</Logo>
	<Watermark>natron-community-small.png</Watermark>
	<AdminTargetDir>/Library/Application Support/Natron/Plugins</AdminTargetDir>
	<TargetDir>/Library/Application Support/Natron/Plugins</TargetDir>
        <RemoteRepositories>
                <Repository>
                        <Url>http://fxarena.net/natron/plugins/packages</Url>
                        <Enabled>1</Enabled>
                        <DisplayName>Natron Community Repository</DisplayName>
                </Repository>
        </RemoteRepositories>
</Installer>
